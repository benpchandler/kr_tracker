{
  "name": "kr-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n \"API,VITE\" -c \"magenta,cyan\" \"npm run dev:server\" \"VITE_USE_BACKEND=true vite\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "server": "node server/index.cjs",
    "dev:server": "nodemon server/index.cjs",
    "dev:client": "VITE_USE_BACKEND=false vite",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src server tests",
    "checks": "npm run lint && node scripts/check-literals.mjs && npm run test:unit",
    "data:export": "node scripts/export-seeds.cjs",
    "data:import": "node scripts/import-seeds.cjs",
    "data:check": "node scripts/export-seeds.cjs && git add -A server/seeds/json",
    "prepare": "husky"
  },
